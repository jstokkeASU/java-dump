package org.javaspring.portfolio;

import java.util.List;

public class Triangle {

	List<Point> points;
	
	public void draw() {
		System.out.println("Point One is at "+points.get(0).getX()+","+points.get(0).getY());
		System.out.println("Point Two is at "+points.get(1).getX()+","+points.get(1).getY());
		System.out.println("Point Three is at "+points.get(2).getX()+","+points.get(2).getY());
	}

	public List<Point> getPoints() {
		return points;
	}

	public void setPoints(List<Point> points) {
		this.points = points;
	}
	

}