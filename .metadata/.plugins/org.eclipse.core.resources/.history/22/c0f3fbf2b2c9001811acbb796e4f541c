package org.javaspring.portfolio;

public class Triangle {
	private Point pointA;
	private Point pointB;
	private Point pointC;
	
	public void draw() {
		System.out.println("Point One is at "+pointA.getX()+","+pointA.getY()+".  Point Two is at "+
				pointB.getX()+","+pointB.getY()".  Point Three is at "+pointC.getX()+","+pointC.getY());
	}

	@Override
	public String toString() {
		return "Triangle [pointA=" + pointA + ", pointB=" + pointB + ", pointC=" + pointC + "]";
	}

	public int getPointA() {
		return pointA;
	}

	public void setPointA(int pointA) {
		this.pointA = pointA;
	}

	public int getPointB() {
		return pointB;
	}

	public void setPointB(int pointB) {
		this.pointB = pointB;
	}

	public int getPointC() {
		return pointC;
	}

	public void setPointC(int pointC) {
		this.pointC = pointC;
	}

}
